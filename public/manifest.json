{
  "name": "universalPlug-in",
  "manifest_version": 2,
  "version": "1.0.0",
  "description": "",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "contextMenus"
  ],
  "icons": {
    "16": "./image/logo.png",
    "48": "./image/logo.png",
    "128": "./image/logo.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}